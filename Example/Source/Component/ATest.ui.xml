<?xml version="1.0" encoding="UTF-8" ?>
<UI xmlns="reactant"
    xmlns:layout="reactant:layout"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="reactant http://schema.brightify.org/reactant/ReactantUI.xsd
                        reactant:layout http://schema.brightify.org/reactant/Layout.xsd"
    type="ATest"
    extend="all"
    anonymous="true"
    rootView="true">

    <ImageView
        layout:edges="super"
        image="falcon"
        visibility="visible"
        clipsToBounds="true"
        layout:compressionPriority="high"
        layout:huggingPriority="low"
        contentMode="scaleAspectFill" />

    <PlainTableView
        cell="ExampleCell3"
        layout:edges="super"
        examples="5">

        <cell anonymous="true">
            <Label
                text="This is nice"
                backgroundColor="white"
                textColor="red"
                layout:fillHorizontally="super inset(20)"
                layout:fillVertically="super inset(0)" />
        </cell>
    </PlainTableView>
</UI>
