<?xml version="1.0" encoding="UTF-8" ?>
<Component xmlns="http://schema.Hyperdrive.tech/ui"
    xmlns:layout="http://schema.Hyperdrive.tech/layout"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://schema.Hyperdrive.tech/ui http://schema.Hyperdrive.tech/ui.xsd
                        http://schema.Hyperdrive.tech/layout http://schema.Hyperdrive.tech/layout.xsd"
    type="ATest"
    extend="all"
    anonymous="true"
    rootView="true">

    <ImageView
        layout:edges="super"
        image="falcon"
        visibility="visible"
        clipsToBounds="true"
        layout:compressionPriority="high"
        layout:huggingPriority="low"
        contentMode="scaleAspectFill" />

    <PlainTableView
        cell="ExampleCell3"
        layout:edges="super"
        examples="5">

        <cell anonymous="true">
            <Label
                text="This is nice"
                backgroundColor="white"
                textColor="red"
                layout:fillHorizontally="super inset(20)"
                layout:fillVertically="super inset(0)" />
        </cell>
    </PlainTableView>
</Component>
